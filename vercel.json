{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/",
            "use": "@vercel/staic"
        }
    ],
    "routes": [
        {
            "src": "/products",
            "dest": "index.js"
        },
        {
            "src": "/products/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "dist/"
        }
      
    ]
}